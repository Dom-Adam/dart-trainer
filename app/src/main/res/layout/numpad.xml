<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp">

        <TextView
            android:id="@+id/ten_label1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/label_text_size"
            app:layout_constraintBottom_toTopOf="@id/ten_button_one"
            app:layout_constraintEnd_toEndOf="@id/ten_button_one"
            app:layout_constraintStart_toStartOf="@id/ten_button_one"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="1st Dart" />

        <TextView
            android:id="@+id/ten_label2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/label_text_size"
            app:layout_constraintBottom_toTopOf="@id/ten_button_two"
            app:layout_constraintEnd_toEndOf="@id/ten_button_two"
            app:layout_constraintStart_toStartOf="@id/ten_button_two"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="2nd Dart" />

        <TextView
            android:id="@+id/ten_label3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/label_text_size"
            app:layout_constraintBottom_toTopOf="@id/ten_button_three"
            app:layout_constraintEnd_toEndOf="@id/ten_button_three"
            app:layout_constraintStart_toStartOf="@id/ten_button_three"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="3rd Dart" />

        <Button
            android:id="@+id/ten_button_one"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:background="@color/button"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_four"
            app:layout_constraintEnd_toStartOf="@id/ten_button_two"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ten_label1"
            tools:text="1" />

        <Button
            android:background="@drawable/button_color_list"
            android:id="@+id/ten_button_two"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_five"
            app:layout_constraintEnd_toStartOf="@id/ten_button_three"
            app:layout_constraintStart_toEndOf="@id/ten_button_one"
            app:layout_constraintTop_toBottomOf="@id/ten_label2"
            tools:text="2" />

        <Button
            android:id="@+id/ten_button_three"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_six"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/ten_button_two"
            app:layout_constraintTop_toBottomOf="@id/ten_label3"
            tools:text="4" />

        <Button
            android:id="@+id/ten_button_four"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_seven"
            app:layout_constraintEnd_toStartOf="@id/ten_button_five"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ten_button_one"
            tools:text="4" />

        <Button
            android:id="@+id/ten_button_five"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_eight"
            app:layout_constraintEnd_toStartOf="@id/ten_button_six"
            app:layout_constraintStart_toEndOf="@id/ten_button_four"
            app:layout_constraintTop_toBottomOf="@id/ten_button_two"
            tools:text="5" />

        <Button
            android:id="@+id/ten_button_six"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_nine"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/ten_button_five"
            app:layout_constraintTop_toBottomOf="@id/ten_button_three"
            tools:text="6" />

        <Button
            android:id="@+id/ten_button_seven"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_ten"
            app:layout_constraintEnd_toStartOf="@id/ten_button_eight"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ten_button_four"
            tools:text="7" />

        <Button
            android:id="@+id/ten_button_eight"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_ten"
            app:layout_constraintEnd_toStartOf="@id/ten_button_nine"
            app:layout_constraintStart_toEndOf="@id/ten_button_seven"
            app:layout_constraintTop_toBottomOf="@id/ten_button_five"
            tools:text="8" />

        <Button
            android:id="@+id/ten_button_nine"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/button_margin"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toTopOf="@id/ten_button_ten"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/ten_button_eight"
            app:layout_constraintTop_toBottomOf="@id/ten_button_six"
            tools:text="9" />

        <Button
            android:id="@+id/ten_button_ten"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:textSize="@dimen/numpad_button_textSize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ten_button_eight"
            tools:text="Enter" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>